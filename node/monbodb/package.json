    "clean": "rm -rf ./build/*",
    "build": "tsc -p tsconfig.prod.json --removeComments"
  },
  "dependencies": {
    "body-parser": "1.19.1",
    "dotenv": "12.0.3",
    "env-var": "7.1.1",
    "express": "4.17.2",
    "express-validator": "6.14.0",
    "mongodb": "4.3.0",
    "mongoose": "6.4.6",
    "nanoid": "3.1.32",
    "tsconfig-paths": "3.12.0",
    "validator": "13.7.0"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/node": "17.0.6",
    "@types/validator": "13.7.1",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "eslint": "8.6.0",
    "ts-node": "10.4.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.5.4"
  }
